package com.list;

import java.util.ArrayList;

public class ListSample {

	public static void main(String[] args) {
		ArrayList<Integer> nos = new ArrayList<Integer>();
		nos.add(10);
		nos.add(5);
		nos.add(3);
		nos.add(2);
		nos.add(19);
		nos.add(78);
		nos.add(5);
		nos.add(10);
		nos.add(1);

		System.out.println(nos.size());// no of elements in hte list
		
	}

}
